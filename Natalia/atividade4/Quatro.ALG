Algoritmo "quatro"

Var
    raio: real
    altura: real
    volume: real
    resultado: real

Inicio
    escreval("Insira o valor do raio da lata:")
    leia(raio)
    escreval("Insira o valor da altura da lata:")
    leia(altura)
    volume <- PI * raio^2 * altura
    escreva("O volume da lata Ã©:", volume)

Fimalgoritmo
