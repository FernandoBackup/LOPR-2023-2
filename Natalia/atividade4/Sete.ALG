Algoritmo "sete"

Var
   cotacaoDolar: real
   dolar: real
   resultado: real

Inicio
   escreval("Inserir o valor atual do dólar:")
   leia(cotacaoDolar)
   escreval("Valor o qual deseja converter:")
   leia(dolar)
   resultado <- dolar / cotacaoDolar
   escreval("O valor em reais equivalem á:", resultado, " doláres")
   


Fimalgoritmo