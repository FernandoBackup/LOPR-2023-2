Algoritmo "dez"

Var
   temperaturaF: real
   temperaturaC: real


Inicio
   escreval("Insira o valor da temperatura em Fahrenheit:")
   leia(temperaturaF)
   temperaturaC <- 5 * (temperaturaF - 32)/9
   escreval("O valor da sua temperatura em Celsius Ã©:", temperaturaC:2:2)

Fimalgoritmo
