Algoritmo "sucessorAntecessor"
//Fernanda Figueredo Berwald
Var
   numA,antecessor,sucessor:inteiro

Inicio
   escreva("Digite um número:")
   leia(numA)
   antecessor <- numA - 1
   sucessor <- numA + 1
   escreval
   escreval("O número informado foi ", numA,", o antecessor é ",antecessor," e o sucessor é ",sucessor)
   
Fimalgoritmo