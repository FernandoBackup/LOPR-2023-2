algoritmo "exercicio1"
var
    numeroSecreto: inteiro
    numeroTentativa: inteiro
inicio
    escreval("Digite um número de 1 a 50 e veja o quanto demora para o computador adivinhar aquele número")
    leia(numeroSecreto)
    repita
        numeroTentativa <- randi(50)
        escreval(numerotentativa)
    ate numeroTentativa = numeroSecreto
    escreval("o ultimo é o número secreto")
fimalgoritmo
