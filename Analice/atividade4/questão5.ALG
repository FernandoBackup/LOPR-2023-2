Algoritmo "questão5"

Var
   ladoQuadrado, area: real

Inicio

      escreval("Vamos calcular a área do quadrado, digite o valor do lado do quadrado: ")
      leia(ladoQuadrado)
      area <- ladoQuadrado ^ 2
      escreval("A área deste quadrado é igual a: ",area," centímetros quadrados.")

Fimalgoritmo
