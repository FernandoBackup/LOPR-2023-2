Algoritmo "questão7"

Var

   cotacao: real
   valorReal: real
   valorEmDolar: real

Inicio
   escreva ("Digite o valor em dólar:")
   leia (valorEmDolar)
   escreva ("Digite a cotação do dólar:")
   leia (cotacao)
   valorReal <- valorEmDolar / cotacao
   escreva ("Essa quantia em real equivale a", valorReal, "dolares.")



Fimalgoritmo


//multiplicar a cotacao pelo dolar para dar em reail
