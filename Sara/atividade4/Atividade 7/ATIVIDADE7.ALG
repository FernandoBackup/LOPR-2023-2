Algoritmo "atividade7"
Var
   cotacaoDolar, dolar, valorReal: real

Inicio
      escreva("Digite o valor em reais: ")
      leia(valorReal)
      escreva("Digite a cotação do dolar do dia: ")
      leia(cotacaoDolar)
      dolar <- valorReal / cotacaoDolar
      escreva("O valor em dolares é $", dolar, ".")


Fimalgoritmo