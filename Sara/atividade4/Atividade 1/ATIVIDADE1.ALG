Algoritmo "Atividade1"

Var
   dataNascimento, nome: caractere

Inicio
      escreva("Digite seu nome: ")
      leia(nome)
      escreva("Digite sua data de nascimento no formato XX/XX/XXXX: ")
      leia(dataNascimento)
      escreva("Seu nome é ", nome, " e voce nasceu em ", dataNascimento, ".")

Fimalgoritmo